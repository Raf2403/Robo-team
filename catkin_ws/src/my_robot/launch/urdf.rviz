# Пример минимального urdf.rviz
Visualization Manager:
  Enabled Displays:
    - Class: rviz/RobotModel
      Description: URDF Model
      Name: RobotModel
      Robot Description: robot_description
      Value: true
  Global Options:
    Background Color: 0; 0; 0
    Fixed Frame: base_link
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 2.0
      Name: Current View
